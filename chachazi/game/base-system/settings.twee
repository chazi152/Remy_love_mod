				<<link "下一个">>
					<<if _npcId is $NPCNameList.length - 1>>
						<<set _npcId to 0>>
					<<else>>
						<<set _npcId++>>
					<</if>>
					<<if _npcList[$NPCName[_npcId].nam] is undefined>>
						<<set _npcId++>>
					<</if>>
					<<replace #npcSettingsMenu>><<npcSettingsMenu>><</replace>>
				<</link>>
