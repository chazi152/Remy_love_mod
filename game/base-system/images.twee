<<widget "img_tf_cow_doggy_idle">>
	<<set $_cow to $transformationParts.cow>>
	<<set $_demon to $transformationParts.demon>>
	<<if isPartEnabled($_cow.horns)>>
		<<set $_hornType to $_cow.horns>>
		<<set $_layer to "layer-horns">>
		<<if isPartEnabled($_demon.horns) and isChimeraEnabled("demoncow", "horns")>>
			<<set $_hornType to 'default-demon'>>
			<<set $_layer to "layer-horns-chimera">>
		<</if>>
		<img @class="$_layer + ' anim-idle-2f'" @src="'img/sex/doggy/active/transformations/cow/horns/'+$_hornType+'.png'">
	<</if>>
	<<if isPartEnabled($_cow.ears)>>
		<img class="layer-horns anim-idle-2f" @src="'img/sex/doggy/active/transformations/cow/ears/'+$_cow.ears+'.png'">
