				<div class="pepper-sprays-dots">
					<<for _i to 1; _i lte $spraymax; _i++>>
						<<if $spray gte _i>>
							<div class="pepper-spray-dot-full"></div>
						<<else>>
							<div class="pepper-spray-dot-empty"></div>
						<</if>>
					<</for>>
