			<<else>>
				<<for _i to 1; _i lte $spraymax; _i++>>
					<<if $spray gte _i>>
						<img src="img/ui/pepperspray.png">
					<<else>>
						<img src="img/ui/emptyspray.png">
					<</if>>
				<</for>>

			<</if>>
